{
  "name": "az-iar/menu",
  "description": "Menu builder",
  "license": "MIT",
  "authors": [
    {
      "name": "Azri Ismail",
      "email": "mazri.iar@gmail.com"
    }
  ],
  "require": {
    "php": "^8.0 | ^8.1 | ^8.2"
  },
  "require-dev": {
    "orchestra/canvas": "^8.1",
    "orchestra/testbench": "^v8.5"
  },
  "autoload": {
    "psr-4": {
      "Inneuron\\Menu\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Inneuron\\Menu\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-autoload-dump": [
      "@php ./vendor/bin/testbench package:discover --ansi"
    ],
    "test": [
      "@php ./vendor/bin/phpunit"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Inneuron\\Menu\\Providers\\MenuServiceProvider"
      ],
      "aliases": {
        "Menu": "Inneuron\\Menu\\Facades\\Menu"
      }
    }
  },
  "config": {
    "sort-packages": true
  }
}
